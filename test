num = 3
# data = list([{'a': None, 'b': None, 'c': None}] * num)
data = [{'a': None, 'b': None, 'c': None} for poop in range(num)]

# data = []
# for i in range(num):
#     data.append({'a': None, 'b': None, 'c': None})


for i in range(num):
    data[i]['a'] = 0.5 * i
    data[i]['b'] = i
    data[i]['c'] = 2 * i


print(data)
